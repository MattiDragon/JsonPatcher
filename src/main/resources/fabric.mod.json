{
  "schemaVersion": 1,
  "id": "jsonpatcher",
  "version": "${version}",
  "name": "JsonPatcher",
  "description": "A tool for patching json resource files easily and with great compatibility.",
  "authors": ["MattiDragon"],
  "contact": {
    "repo": "https://github.com/MattiDragon/JsonPatch"
  },
  "license": "MIT",
  "icon": "assets/jsonpatch/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.mattidragon.jsonpatcher.JsonPatcher"
    ]
  },
  "accessWidener" : "jsonpatcher.accesswidener",
  "mixins": [
    "jsonpatcher.mixins.json",
    "client.jsonpatcher.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric": "*",
    "minecraft": "1.20.1"
  }
}
